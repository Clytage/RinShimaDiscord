{
    "name": "project-template",
    "version": "1.0.0",
    "description": "A template to generate well-configured project base for Clytage.",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "npm run lint && npm run compile",
        "compile": "tsc --build tsconfig.json",
        "lint": "eslint . --ignore-path .gitignore --ext .ts",
        "lint:fix": "npm run lint -- --fix",
        "start": "node --es-module-specifier-resolution=node -r dotenv/config -r tsconfig-paths/register .",
        "start:dev": "rimraf ./dist && npm run compile && npm start"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Clytage/project-template.git"
    },
    "author": "Clytage <support@clytage.org>",
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/Clytage/project-template/issues"
    },
    "homepage": "https://github.com/Clytage/project-template#readme",
    "engines": {
        "node": ">=16.6.0",
        "npm": ">=7.0.0"
    },
    "eslintConfig": {
        "extends": "@clytage-pkg/eslint-config/typescript",
        "ignorePatterns": "dist/*"
    },
    "imports": {
        "#rin/*": "./dist/*"
    },
    "devDependencies": {
        "@clytage-pkg/eslint-config": "^1.3.0",
        "@types/node": "^17.0.23",
        "@typescript-eslint/eslint-plugin": "^5.30.0",
        "@typescript-eslint/parser": "^5.30.0",
        "eslint": "^8.18.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "@typegoose/typegoose": "^9.10.0",
        "date-fns": "^2.28.0",
        "discord.js": "^13.8.1",
        "dotenv": "^16.0.1",
        "got": "^12.1.0",
        "mongoose": "^6.3.5",
        "pino": "^8.1.0",
        "pino-pretty": "^8.1.0",
        "tree-kill": "^1.2.2",
        "tsconfig-paths": "^4.0.0"
    }
}
